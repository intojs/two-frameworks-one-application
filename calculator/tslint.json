{
  "extends": [
    "tslint:latest"
  ],
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "object-literal-sort-keys": [
      true,
      "match-declaration-order"
    ]
  }
}
